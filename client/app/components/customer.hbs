<LinkTo @route="user" class="item-selected">
<div class="align-items-center text-center mb-3">
    <img src={{this.src}} class="rounded-circle mb-2" style="width: 75px; height: 75px; object-fit: cover;" alt="User Photo">
    <h5 class="text-dark font-weight-bold">
        {{this.fullname}}
    </h5>
    <Badge @isFive="true" />
</div>
</LinkTo>